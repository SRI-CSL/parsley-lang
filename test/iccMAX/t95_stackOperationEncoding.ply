// Table 95: Stack Operation Encoding, page 104 iccMAX spec
// Valid for Table 97: Matrix Stack Operation Encoding, page 104 iccMAX spec

type soe_type = {
    // operation signature
    op_signature: [byte],

    // Table 95: number of extra elements selector S
    // Table 97: index of last matrix row S
    s: int,

    // Table 95: number of extra times selector T
    // Table 97: index of last matrix column T
    t: int
}

format {
    StackOperationEncoding soe {soe_type} :=
        soe_op_signature = (Byte^4)
        soe_s = UInt16<endian=endian::Big()>
        soe_t = UInt16<endian=endian::Big()>

        {
            soe.op_signature := soe_op_signature;
            soe.s := soe_s;
            soe.t := soe_t
        }
}
