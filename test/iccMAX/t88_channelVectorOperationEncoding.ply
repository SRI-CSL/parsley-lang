// Table 88 of iccMAX documentation, Channel Vector Operation Encoding
// page 101 of the specification

type cvoe_type = {
    // operation signature
    op_signature: [byte],
    // starting index
    s: int,
    // additional count from the start
    t: int
}

format {
    ChannelVectorOperationEncoding cvoe {c: cvoe_type} :=
        cvoe_op_signature = (Byte^4)
        cvoe_s = UInt16<endian=endian::Big()>
        cvoe_t = UInt16<endian=endian::Big()>

        {
            cvoe.c.op_signature := cvoe_op_signature;
            cvoe.c.s := cvoe_s;
            cvoe.c.t := cvoe_t
        }
}
